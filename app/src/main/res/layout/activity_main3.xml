<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FF2626"
    tools:context=".MainActivity3">

    <TextView
        android:id="@+id/textViewMusk"
        android:layout_width="193dp"
        android:layout_height="167dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="53dp"
        android:layout_marginEnd="109dp"
        android:layout_marginBottom="84dp"
        android:text="@string/textAboutMusk"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewMusk"
        app:layout_constraintVertical_bias="0.115" />

    <ImageView
        android:id="@+id/imageViewMusk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sk_rmbillede_2023_03_14_111624" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="10dp"
        android:onClick="homePressed"
        android:text="Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/nextPage"
        android:layout_width="118dp"
        android:layout_height="63dp"
        android:layout_marginBottom="9dp"
        android:onClick="seeMore"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/textViewMusk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewMusk" />
</androidx.constraintlayout.widget.ConstraintLayout>